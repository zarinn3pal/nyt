var url = "https://api.nytimes.com/svc/topstories/v2/sports.json";
url += '?' + $.param({
  'api-key': "d66a121bec3c400c9cb051328f9d33ea"
});
$.ajax({
  url: url,
  method: 'GET',
}).done(function(result) {
  console.log(result);
}).fail(function(err) {
  throw err;
});



